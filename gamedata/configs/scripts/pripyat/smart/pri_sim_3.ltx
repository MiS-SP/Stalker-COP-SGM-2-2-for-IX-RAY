[smart_terrain]
squad_id = 8
max_population = 2
respawn_params = respawn@pri_alfa_sim_3

[respawn@pri_alfa_sim_3]
pri_alfa_force_novice
pri_alfa_force_regular
pri_alfa_force_experienced
pri_alfa_force_veteran
pri_alfa_force_master

[pri_alfa_force_novice]
spawn_squads = pri_alfa_squad_1_novice
spawn_num = {+opt_activate_alfa_force =check_actor_rank(0:100)} 1, 0

[pri_alfa_force_regular]
spawn_squads = pri_alfa_squad_1_regular
spawn_num = {+opt_activate_alfa_force =check_actor_rank(100:200)} 1, 0

[pri_alfa_force_experienced]
spawn_squads = pri_alfa_squad_1_experienced
spawn_num = {+opt_activate_alfa_force =check_actor_rank(200:350)} 1, 0

[pri_alfa_force_veteran]
spawn_squads = pri_alfa_squad_1_veteran
spawn_num = {+opt_activate_alfa_force =check_actor_rank(350:600)} 1, 0

[pri_alfa_force_master]
spawn_squads = pri_alfa_squad_1_master
spawn_num = {+opt_activate_alfa_force =check_actor_rank(600)} 1, 0