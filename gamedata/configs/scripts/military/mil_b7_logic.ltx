;Логика "База группировки Рассвет"

[walker@def_guarder]
use_camp = false
gather_items_enabled = false
help_wounded_enabled = true
corpse_detection_enabled = false
precond_timer = {!actor_enemy =actor_has_weapon} true, false

[camper@def_guarder]
use_camp = false
gather_items_enabled = false
help_wounded_enabled = true
corpse_detection_enabled = false

[remark@def_guarder]
anim = threat
target = story|actor
on_info = {-mil_b7_base_guard =actor_has_weapon} %+mil_b7_base_guard =play_sound_theme(zat_a2_base_guard)%

;

[logic@mil_b7_leader]
active = walker@leader
suitable = {=check_npc_name(mil_b7_base_leader)} true
prior = 200

[walker@leader]
path_walk = leader_walk
path_look = leader_look
use_camp = false
gather_items_enabled = false
help_wounded_enabled = false
corpse_detection_enabled = false
meet = meet@b7_leader
invulnerable = {+mil_defence_b7_start} false, {-mil_alt_storyline_commenced} true, false

[meet@b7_leader]
use = {!actor_enemy !actor_has_weapon !has_enemy} true, false
allow_break = false
trade_enable = false

;

[logic@mil_b7_chief]
active = walker@chief
suitable = {=check_npc_name(mil_b7_base_chief)} true
prior = 190

[walker@chief]
path_walk = chief_walk
path_look = chief_look
use_camp = false
gather_items_enabled = false
help_wounded_enabled = false
corpse_detection_enabled = false
meet = meet@b7_chief
invulnerable = {+mil_defence_b7_start} false, {-mil_alt_storyline_commenced} true, false

[meet@b7_chief]
use = {-mil_b7_base_chief_hello !actor_enemy =dist_to_actor_le(5)} self, {!actor_enemy !actor_has_weapon !has_enemy} true, false
allow_break = false
trade_enable = false

;

[logic@mil_b7_guarder_1]
