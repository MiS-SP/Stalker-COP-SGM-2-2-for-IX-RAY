[smart_terrain];zat_sim_10
squad_id = 26
max_population = 2
respawn_params = respawn@zat_alfa_sim_10

[respawn@zat_alfa_sim_10]
zat_alfa_force_novice
zat_alfa_force_regular
zat_alfa_force_experienced
zat_alfa_force_veteran
zat_alfa_force_master

[zat_alfa_force_novice]
